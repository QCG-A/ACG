--Quest--
local Quest = Window:MakeTab({
	Name = "Quest",
	PremiumOnly = false
})

Quest:AddButton({
	Name = "Amiya Quest",
	Callback = function()
        game:GetService("ReplicatedStorage"):WaitForChild("CheckEvents"):WaitForChild("BossQuest"):FireServer()
        wait(1)
        if game:GetService("Players").LocalPlayer.BossQuest.Completed.Value == true then
            game:GetService("ReplicatedStorage"):WaitForChild("CheckEvents"):WaitForChild("BossQuestReward"):FireServer()
        wait(2)
        if game:GetService("Players").LocalPlayer.BossQuest.DoingQuest.Value == false and game:GetService("Players").LocalPlayer.BossQuest.Completed.Value == false then
            OrionLib:MakeNotification({
                Name = "Hey!",
                Content = "The Amiya quest is not active, it looks like the cooldown hasn't finished. Please wait a moment and click again.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
end    
})

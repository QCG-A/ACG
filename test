local Misc = Window:MakeTab({
	Name = "Misc",
	PremiumOnly = false
})

Misc:AddButton({
	Name = "Kill all",
	Callback = function()
        for i,d in pairs(game.Workspace:GetDescendants()) do
            if d.ClassName == 'Model' and d.Parent.Name ~= game.Players.LocalPlayer.Name then
                   local args = {
	{
		d
	},
	{
		Attributes = {
			Ragdoll = false,
			Daze = false,
			ShowVisual = false,
			Effects = {
				Visual = {
					["Common/hit_punch_1"] = {}
				}
			},
			IgnoreBlock = false,
			IgnoreIframe = false,
			MultipleNpcDmg = 1,
			Stun = 1,
			Guardbreak = false,
			Knockback = {
				Duration = 0.3,
				Direction = vector.create(-0.11328146606683731, -6.295117316312826e-08, 0.9935629367828369)
			},
			Damage = 6000
		},
		Properties = {
			DebounceDuration = 0.3,
			PartOffset = CFrame.new(0, 0, -3, 1, 0, 0, 0, 1, 0, 0, 0, 1),
			Duration = 0.25,
			ReferencePart = getgenv().KillMob,
			Size = 3
		}
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("RegisterHit"):FireServer(unpack(args))
           end
  	end    
	  end
})


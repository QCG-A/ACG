function autopickupBSTicket()
    while _G.autopickupBSTicket == true do
    wait(0.2)
    game:GetService("ReplicatedStorage"):WaitForChild("CheckEvents"):WaitForChild("GetBSInv"):FireServer();
end
end

function autojoinHe()
local player = game.Players.LocalPlayer
        local playerui = player.PlayerGui
        local Heatcliff = playerui.ErlkingNotice
		while _G.autojoinHe do
            wait(1)
            if Heatcliff.Enabled == true then
           game:GetService("ReplicatedStorage"):WaitForChild("DungeonSpawn"):WaitForChild("ErlkingSpawn"):FireServer()
        end
		end
    end

function susanoounitalarm()
    local player = game.Players.LocalPlayer
        local playerui = player.PlayerGui
        local susanoo = playerui.MenuGUI.SusanooIndicator
        while _G.susanoounitalarm do
            wait(1)
            if susanoo.Visible == true then
           local Alarm = "http://www.roblox.com/asset/?id=449013696"
               local Sound = Instance.new("Sound");Sound.Name = "Alarm";Sound.SoundId = Alarm
               Sound.Volume = 10;Sound.Looped = true;Sound.Archivable = false;Sound.Parent = game.Workspace;Sound:Play();
                break
            end
        end
    end

        function hpall()
            while _G.hpall == true do
                wait(0.2)
                local args = {
                    [1] = "SelfHeal"
                }
                
                game:GetService("ReplicatedStorage"):WaitForChild("Kaltsit"):WaitForChild("KaltsitEvent"):FireServer(unpack(args))
            end
        end



        WindUI:Notify({
            Title = "Hi!",
            Content = "If you use the script on a public server, you risk getting banned, so please use it carefully on your own server.",
            Duration = 5,
        })


--Farming--

local Farming = Window:MakeTab({
	Name = "Farming",
	PremiumOnly = false
})

Farming:AddDropdown({
	Name = "Select Mob",
	Default = "1",
	Options = {"OriginiumSlug", "OriginiumSlug2", "ReunionSoldier", "EliteSoldier", "Avenger", "HexedColossus", "TheGramophone", "LeithanianWhisperer"},
	Callback = function(Value)
		getgenv().KillMob = Value;
	end    
})

Farming:AddButton({
	Name = "Kill Mob",
	Callback = function()
        local args = {
	{
		workspace:WaitForChild(getgenv().KillMob)
	},
	{
		Attributes = {
			Ragdoll = false,
			Daze = false,
			ShowVisual = false,
			Effects = {
				Visual = {
					["Common/hit_punch_1"] = {}
				}
			},
			IgnoreBlock = false,
			IgnoreIframe = false,
			MultipleNpcDmg = 1,
			Stun = 1,
			Guardbreak = false,
			Knockback = {
				Duration = 0.3,
				Direction = vector.create(-0.11328146606683731, -6.295117316312826e-08, 0.9935629367828369)
			},
			Damage = 6000
		},
		Properties = {
			DebounceDuration = 0.3,
			PartOffset = CFrame.new(0, 0, -3, 1, 0, 0, 0, 1, 0, 0, 0, 1),
			Duration = 0.25,
			ReferencePart = getgenv().KillMob,
			Size = 3
		}
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("RegisterHit"):FireServer(unpack(args))
end
})

Farming:AddDropdown({
	Name = "Select Boss",
	Default = "1",
	Options = {"Skullshatterer", "New Nothing There", "MenosGrande", "Chatterbox", "Doro", "Hateful Avenger", "Mudrock", "Mahoraga", "The Black Silence", "RolandP3", "RolandP4", "TexasBoss", "Kali, The Red Mist", "ErlkingHeathcliff", "CatherineBoss", "ErlkingDullahan", "Qu, The Last Head of Kowloong"},

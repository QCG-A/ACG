--kick--
local gameid = game.PlaceId

if not (gameid == 18576660276) then
    game.Players.LocalPlayer:Kick("This script is for ACGs only; otherwise, the script will not run. SkiBidi!")
end

-- Anti AFK --
task.spawn(function()
    if not getgenv().AntiAfk == true then getgenv().AntiAfk = true;
        while true do task.wait(1100);
            pcall(function()
                game:GetService("VirtualInputManager"):SendKeyEvent(true,"RightBracket",false,game);
            end)
        end
    end
end)
--Game Loader--
local GameLoader = loadstring(game:HttpGet(('https://pastebin.com/raw/k4sNWFMp')))()

local player = game.Players.LocalPlayer
--Version--
getgenv().CurrentVersion = "0.01 / BETA";

--Orion Library--
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/DKDKem/Test/refs/heads/main/test1.lua')))()

local Window = OrionLib:MakeWindow({Name = " ♦️ Library Of ACG / ACG Operation Version "..tostring(getgenv().CurrentVersion).." ♦️ ", HidePremium = false, SaveConfig = true, ConfigFolder = "LCG"})

--music--
local Miku = "http://www.roblox.com/asset/?id=8247775255"
               local Miku1 = Instance.new("Sound");Miku1.Name = "Miku";Miku1.SoundId = Miku
               Miku1.Volume = 10;Miku1.Looped = false;Miku1.Archivable = false;Miku1.Parent = game.Workspace;Miku1:Play();
               
--Values
_G.autopickupitems = false
_G.autopickupPresentBox = false
_G.autopickupBSTicket = false
_G.susanoounitalarm = false
_G.hpall = false
--Functions

function autopickupitems()
    while _G.autopickupitems == true do
        wait(1.5)
		local char = game:GetService('Players').LocalPlayer.Character
        game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
            char = body end)
          for i,v in pairs(workspace:GetDescendants()) do
           if v:IsA('Tool') then
            char.Humanoid:EquipTool(v)
           end
        end
	end  
end

function autopickupPresentBox()
    task.spawn(function()
        while _G.autopickupPresentBox == true do
            wait(0.6)
            pcall(function()
		local PresentBox = game:GetService("Workspace"):FindFirstChild("PresentBox"):FindFirstChild("Main");
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(PresentBox.Position - PresentBox.CFrame.UpVector * -4,PresentBox.Position)
       wait(0.6)
        fireproximityprompt(workspace.PresentBox.Main.ProximityPrompt)
        end)
    end
end)
end

function AutoHeathcliff()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/QCG-A/ACG/refs/heads/main/autoBoss/AutoHeathcliff.lua"))();
end

function autopickupBSTicket()
    while _G.autopickupBSTicket == true do
    wait(0.2)
    game:GetService("ReplicatedStorage"):WaitForChild("CheckEvents"):WaitForChild("GetBSInv"):FireServer();
end
end

function susanoounitalarm()
    local player = game.Players.LocalPlayer
        local playerui = player.PlayerGui
        local susanoo = playerui.MenuGUI.SusanooIndicator
        while _G.susanoounitalarm do
            wait(1)
            if susanoo.Visible == true then
           local Alarm = "http://www.roblox.com/asset/?id=449013696"
               local Sound = Instance.new("Sound");Sound.Name = "Alarm";Sound.SoundId = Alarm
               Sound.Volume = 10;Sound.Looped = true;Sound.Archivable = false;Sound.Parent = game.Workspace;Sound:Play();
                break
            end
        end
    end

        function hpall()
            while _G.hpall == true do
                wait(0.2)
                local args = {
                    [1] = "SelfHeal"
                }
                
                game:GetService("ReplicatedStorage"):WaitForChild("Kaltsit"):WaitForChild("KaltsitEvent"):FireServer(unpack(args))
            end
        end



--Values
_G.autopickupitems = false
_G.autopickupPresentBox = false
_G.autopickupBSTicket = false
_G.susanoounitalarm = false
_G.hpall = false
--Functions

function autopickupitems()
    while _G.autopickupitems == true do
        wait(1.5)
		local char = game:GetService('Players').LocalPlayer.Character
        game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
            char = body end)
          for i,v in pairs(workspace:GetDescendants()) do
           if v:IsA('Tool') then
            char.Humanoid:EquipTool(v)
           end
        end
	end  
end

function autopickupPresentBox()
    task.spawn(function()
        while _G.autopickupPresentBox == true do
		wait(0.1)
            pcall(function()
		local PresentBox = game:GetService("Workspace"):FindFirstChild("PresentBox"):FindFirstChild("Main");
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(PresentBox.Position - PresentBox.CFrame.UpVector * -4,PresentBox.Position)

        fireproximityprompt(workspace.PresentBox.Main.ProximityPrompt)
        end)
    end
end)
end

function AutoHeathcliff()
			task.spawn(function()
        while _G.AutoHeathcliff == true do
            wait(0.6)
			-- Teleport --
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1772.07983, 2227.46191, 1560.42493, 0.998259544, 1.67380454e-09, 0.0589740612, -7.31890659e-10, 1, -1.59932654e-08, -0.0589740612, 1.59222662e-08, 0.998259544);
            pcall(function()
		             for _, x in ipairs(workspace:GetChildren()) do
                if string.find(x.Name, "ErlkingHeathcliff") then -- Heathcliff Raid 1 Phase

					local args = {
	{
		x
	},
	{
		Attributes = {
			Ragdoll = false,
